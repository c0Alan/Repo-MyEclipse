<?xml version="1.0" encoding="UTF-8"?>
 <Methods>
  	<Method name="get" return="Object" tips="通过控件id得到控件对象，此方法相当于Ext.getCmp方法">
  	  <Description><![CDATA[通过控件id得到控件对象，此方法相当于Ext.getCmp方法
  		<pre>
Artery.get("id");
  		</pre>]]></Description>
  		<Parameters>
  			<Param>id：String 控件的id</Param>
  		</Parameters>
  		<Return>object:返回控件对象</Return>
  	</Method>
  	<Method name="getWin" return="Object" tips="当参数iframeId没有时，可得到父窗口的Artery对象...">
  	  <Description><![CDATA[当参数iframeId没有时，可得到父窗口的Artery对象，否则可得到指定iframeId的iframe的Artery对象
  		<pre>
Artery.getWin("iframeId:String");
  		</pre>]]></Description>
  		<Parameters>
  			<Param>iframeId:String 可选的，iframe的id</Param>
  		</Parameters>
  		<Return>Object 返回Artery对象</Return>
  	</Method>
  	<Method name="alertMsg" return="void" tips="提示对象话框">
  	  <Description><![CDATA[提示对象话框
  		<pre>
示例：Artery.alertMsg('提示信息','测试内容');
  		</pre>]]></Description>
  		<Parameters>
  			<Param>titles:String 对话框的标题</Param>
  			<Param>text:String 对话框的内容</Param>
  			<Param value="function(){}">callback:function 可选的，点击对话框的确定按钮后的回调函数</Param>
  		</Parameters>
  		<Return>void</Return>
  	</Method>
  	<Method name="getWindow" return="object" tips="得到当前窗口的Window对象">
  	  <Description><![CDATA[得到当前窗口的Window对象
  		<br>Artery.getWindow();]]></Description>
  		<Parameters>
  			<Param>无</Param>
  		</Parameters>
  		<Return>object:返回Window对象</Return>
  	</Method>
  	<Method name="confirmMsg" return="void" tips="确认对象话框">
  	  <Description><![CDATA[确认对象话框
  		<br>示例：Artery.confirmMsg("删除","确定要删除吗？",function(btn){<br>		if(btn　==　"yes"){<br>　　　　//todo<br>　　}<br>})]]></Description>
  		<Parameters>
  			<Param>titles:String 对话框的标题</Param>
  			<Param>text:String 对话框的内容</Param>
  			<Param value='function(btn){if(btn=="yes"){\r\n//todo\r\n}}'>callback:Function 点击对话框的按钮后的回调函数</Param>
  		</Parameters>
  		<Return>viod</Return>
  	</Method>
  	<Method name="promptMsg" return="void" tips="提示对象话框">
  	  <Description><![CDATA[提示对象话框
  		<br>Artery.promptMsg("输入","请输入名称：",function(btn){<br>		if(btn　==　"yes"){<br>　　　　//todo<br>　　}<br>},"测试")]]></Description>
  		<Parameters>
  			<Param>titles:String 对话框的标题</Param>
  			<Param>text:String 对话框的内容</Param>
  			<Param value='function(btn){if(btn=="yes"){\r\n//todo\r\n}}'>callback:Function 点击对话框的按钮后的回调函数</Param>
  			<Param>value:String 提示对话框的默认值</Param>
  		</Parameters>
  		<Return>viod</Return>
  	</Method>
